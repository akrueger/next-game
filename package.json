{
  "name": "next-game",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "ng serve",
    "start": "node ./server/main.js",
    "build": "ng build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "lint-staged",
    "heroku-postbuild": "ng build --prod",
    "db:backup:schema": "pg_dump --file 'db/backups/next-game-schema.sql' --host 'localhost' --port '5432' --username 'akrueger' --no-password --verbose --format=p --schema-only --no-owner --create --clean --no-privileges --no-tablespaces --no-unlogged-table-data 'next-game'",
    "db:backup:data": "pg_dump --file 'db/backups/next-game-data.sql' --host 'localhost' --port '5432' --username 'akrueger' --no-password --verbose --format=p --data-only --no-privileges --no-tablespaces --no-unlogged-table-data 'next-game'",
    "db:restore:schema": "psql -f db/backups/next-game-schema.sql",
    "db:restore:data": "psql -d next-game -f db/backups/next-game-data.sql"
  },
  "lint-staged": {
    "*.+(ts|js|json)": [
      "prettier --write"
    ],
    "*.ts": [
      "tslint -p ./tsconfig.json"
    ]
  },
  "dependencies": {
    "@angular/animations": "^8.0.0-rc.3",
    "@angular/cdk": "^8.0.0-rc.0",
    "@angular/common": "^8.0.0-rc.3",
    "@angular/compiler": "^8.0.0-rc.3",
    "@angular/core": "^8.0.0-rc.3",
    "@angular/forms": "^8.0.0-rc.3",
    "@angular/platform-browser": "^8.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^8.0.0-rc.3",
    "@angular/router": "^8.0.0-rc.3",
    "@fullcalendar/core": "^4.1.0",
    "@fullcalendar/daygrid": "^4.1.0",
    "@fullcalendar/interaction": "^4.1.0",
    "@ngrx/effects": "^8.0.0-beta.1",
    "@ngrx/entity": "^8.0.0-beta.1",
    "@ngrx/router-store": "^8.0.0-beta.1",
    "@ngrx/store": "^8.0.0-beta.1",
    "auth0-js": "^9.5.1",
    "boardgamegeek": "^0.5.5",
    "compression": "^1.7.2",
    "core-js": "^3.0.1",
    "express": "^4.16.4",
    "express-http-to-https": "^1.1.4",
    "express-jwt": "^5.3.1",
    "express-pino-logger": "^4.0.0",
    "helmet": "^3.12.0",
    "jwks-rsa": "^1.2.1",
    "massive": "^5.7.7",
    "nanoid": "^2.0.1",
    "ngrx-store-localstorage": "^7.0.0",
    "normalize.css": "^8.0.0",
    "pg-connection-string": "^2.0.0",
    "pino": "^5.12.3",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.2",
    "rxjs": "^6.5.2",
    "wicg-inert": "^2.1.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.0-rc.3",
    "@angular/cli": "^8.0.0-rc.3",
    "@angular/compiler-cli": "^8.0.0-rc.3",
    "@angular/language-service": "^8.0.0-rc.3",
    "@ngrx/store-devtools": "^8.0.0-beta.1",
    "@types/node": "~11.13.0",
    "codelyzer": "~5.0.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.0.1",
    "lint-staged": "^8.1.5",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-logger": "^0.2.3",
    "pino-pretty": "^3.0.0",
    "prettier": "^1.12.1",
    "ts-node": "~8.0.3",
    "tslint": "~5.15.0",
    "typescript": "~3.4.0"
  }
}
