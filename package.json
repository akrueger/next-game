{
  "name": "next-game",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "ng serve",
    "start": "node ./server/main.js",
    "build": "ng build",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "lint-staged",
    "heroku-postbuild": "ng build --prod",
    "db:backup:schema": "pg_dump --file 'db/backups/next-game-schema.sql' --host 'localhost' --port '5432' --username 'akrueger' --no-password --verbose --format=p --schema-only --no-owner --create --clean --no-privileges --no-tablespaces --no-unlogged-table-data 'next-game'",
    "db:backup:data": "pg_dump --file 'db/backups/next-game-data.sql' --host 'localhost' --port '5432' --username 'akrueger' --no-password --verbose --format=p --data-only --no-privileges --no-tablespaces --no-unlogged-table-data 'next-game'",
    "db:restore:schema": "psql -f db/backups/next-game-schema.sql",
    "db:restore:data": "psql -d next-game -f db/backups/next-game-data.sql"
  },
  "lint-staged": {
    "*.+(ts|js|json)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/animations": "~8.2.8",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "~8.2.8",
    "@angular/compiler": "~8.2.8",
    "@angular/core": "~8.2.8",
    "@angular/forms": "~8.2.8",
    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "~8.2.8",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/entity": "^8.3.0",
    "@ngrx/router-store": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "auth0-js": "^9.11.3",
    "boardgamegeek": "^0.5.5",
    "compression": "^1.7.4",
    "core-js": "^3.0.1",
    "express": "^4.17.1",
    "express-http-to-https": "^1.1.4",
    "express-jwt": "^5.3.1",
    "express-pino-logger": "^4.0.0",
    "helmet": "^3.21.1",
    "jwks-rsa": "^1.6.0",
    "massive": "^6.1.0",
    "nanoid": "^2.1.1",
    "ngrx-store-localstorage": "^8.0.0",
    "normalize.css": "^8.0.1",
    "pg-connection-string": "^2.1.0",
    "pino": "^5.13.4",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "wicg-inert": "^3.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "~8.2.8",
    "@angular/language-service": "~8.2.8",
    "@ngrx/store-devtools": "^8.3.0",
    "@types/node": "~8.9.4",
    "husky": "^3.0.7",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.0.1",
    "lint-staged": "^9.4.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-logger": "^0.2.4",
    "pino-pretty": "^3.2.1",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "typescript": "~3.5.3"
  }
}
